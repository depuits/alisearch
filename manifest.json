 
{
	"manifest_version": 2,
	"name": "AliSearch",
	"version": "1.0",

	"description": "Search helping tools for aliexpress.com.",

	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png"
	},

	"permissions": [
		"*://*.aliexpress.com/*", 
		"activeTab"
	],

	"content_scripts": [{
		"matches": ["*://*.aliexpress.com/*"],
		"js": ["main.js"],
		"css": ["main.css"]
	}]
}
